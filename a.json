[
  {
    "id": "rule-1",
    "upstream": {
      "url": "https://httpbin.org/anything"
    },
    "match": {
      "url": "http://<[^/]+>/authenticator/noop/authorizer/allow/mutator/noop",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH"]
    },
    "authenticators": [
      {
        "handler": "noop"
      }
    ],
    "authorizer": {
      "handler": "allow"
    },
    "mutators": [
      {
        "handler": "noop"
      }
    ]
  }
]
