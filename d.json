[
  {
  "id": "kratos-registration",
  "match": {
    "url": "http://<[^/]+>/self-service/registration.*",
    "methods": ["GET", "POST"]
  },
  "authenticators": [
    {
      "handler": "anonymous"
    }
  ],
  "authorizer": {
    "handler": "allow"
  },
  "mutators": [
    {
      "handler": "noop"
    }
  ],
  "upstream": {
    "url": "http://kratos.nguyenminhduy.click"
  }
}
]
